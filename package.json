{
  "name": "leo-cli",
  "version": "0.1.0",
  "description": "A scaffolding tool for study",
  "keywords": [
    "scaffolding",
    "generator",
    "cli",
    "typescript",
    "jest",
    "node"
  ],
  "homepage": "https://github.com/leozhang007/leo-cli",
  "bugs": {
    "url": "https://github.com/leozhang007/leo-cli/issues"
  },
  "repository": {},
  "license": "MIT",
  "author": {
    "name": "leozhang",
    "email": "117000082@qq.com"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": "bin/leo",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint .",
    "compile": "tsc",
    "build": "run-s clean lint compile",
    "test": "jest",
    "cov": "jest --coverage",
    "prepare": "run-s build test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --fix"
  },
  "eslintConfig": {
    "extends": "standard-ts",
    "parserOptions": {
      "project": "tsconfig.eslint.json"
    },
    "ignorePatterns": [
      "lib"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "dependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "cac": "^6.6.1",
    "chalk": "^4.1.0",
    "env-paths": "^2.2.0",
    "extract-zip": "^2.0.1",
    "fast-glob": "^3.2.4",
    "husky": "^4.3.6",
    "ini": "^1.3.6",
    "lint-staged": "^10.5.3",
    "lodash": "^4.17.20",
    "mwa": "^0.1.0",
    "node-fetch": "^3.0.0-beta.7",
    "ora": "^5.1.0",
    "prompts": "^2.4.0",
    "semver": "^7.3.4",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "@types/ini": "^1.3.30",
    "@types/jest": "^26.0.17",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.10",
    "@types/prompts": "^2.0.9",
    "@types/semver": "^7.3.4",
    "@types/validate-npm-package-name": "^3.0.2",
    "eslint": "^7.15.0",
    "eslint-config-standard-ts": "^19.0.1",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=10.12"
  },
  "publishConfig": {
    "tag": "alpha",
    "registry": "https://registry.npmjs.org"
  }
}
